{
	"name": "flutterfactoftheday_bot",
	"version": "0.1.0",
	"description": "A stupid twitch bot",
	"main": "bot.js",
	"repository": {
		"url": "https://git.maxemiliang.cloud/maxemiliang/flutterfactbot"
	},
	"scripts": {
		"watch": "npx tsc -w",
		"build": "npm run lint && npx tsc",
		"build:docker": "docker build . -t ffotdbot",
		"start": "node -r dotenv/config dist/bot.js",
		"start:dev": "npx nodemon -r dotenv/config dist/bot.js",
		"start:docker": "docker-compose up --build",
		"lint": "npx tslint -c tslint.json 'src/**/*.ts'"
	},
	"author": "Maxemilian Gr√∂nblom <maxemiliang@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.19.2",
		"dotenv": "^8.2.0",
		"lokijs": "^1.5.8",
		"tmi.js": "^1.5.0",
		"typescript": "^3.9.2"
	},
	"devDependencies": {
		"@types/lokijs": "^1.5.3",
		"@types/node": "^14.0.1",
		"@types/tmi.js": "^1.4.0",
		"nodemon": "^2.0.3",
		"prettier": "^2.0.5",
		"tslint": "^6.1.2",
		"tslint-config-prettier": "^1.18.0"
	}
}
